{
    "name": "sormagec/laravel-activitylog-mongo",
    "description": "A very simple activity logger to monitor the users of your website or application (for MongoDB)",
    "homepage": "https://github.com/bsormagec/laravel-activitylog",
    "keywords":
    [
        "spatie",
        "log",
        "user",
        "activity",
        "laravel",
        "sormagec",
        "mongodb"
    ],
    "authors": [
        {
            "name": "Burak Sormage√ß",
            "email": "burak@sormagec.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2",
        "illuminate/config": "~5.7.0|~5.8.0",
        "illuminate/database": "~5.7.0|~5.8.0",
        "illuminate/support": "~5.7.0|~5.8.0",
        "spatie/string": "^2.1",
        "jenssegers/mongodb": "^3.5"
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "^7.5|^8.0",
        "orchestra/testbench": "~3.8.0",
        "scrutinizer/ocular": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Activitylog\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Activitylog\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Activitylog\\ActivitylogServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
